{
    "name": "fixted",
    "version": "4.2.0",
    "description": "A simple way to populate Sails.js v1 models with data fixtures for testing purposes. Originally based on Barrels, by Ruslan Bredikhin.",
    "keywords": [
        "sails",
        "models",
        "fixtures",
        "testing",
        "database",
        "orm",
        "waterline"
    ],
    "dependencies": {
        "async": "~3.2.4",
        "lodash": "~4.17.21"
    },
    "devDependencies": {
        "chai": "~4.3.7",
        "eslint": "~8.38.0",
        "mocha": "~10.2.0",
        "pre-commit": "~1.2.2",
        "sails": "~1.5.4",
        "sails-disk": "~2.1.2",
        "sails-hook-orm": "~4.0.2"
    },
    "peerDependencies": {
        "sails": "1.x"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/neonexus"
        }
    ],
    "main": "index.js",
    "scripts": {
        "lint": "./node_modules/eslint/bin/eslint.js . --max-warnings=0 --report-unused-disable-directives && echo '✔  Your .js files look good.'",
        "test": "npm run lint && NODE_ENV=test PORT=1338 mocha -R spec --ui bdd --timeout 5000 \"test/**/*.test.js\" && echo '✔ Done ✔\n'"
    },
    "pre-commit": [
        "lint",
        "test"
    ],
    "repository": {
        "type": "git",
        "url": "git://github.com/neonexus/fixted.git"
    },
    "author": {
        "name": "NeoNexus DeMortis",
        "email": "neonexus.demortis@gmail.com"
    },
